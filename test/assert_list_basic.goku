# Test basic list functionality
let my_list = [1, 2, 3, 4, 5];
print my_list;

# Test head and tail
let first = head(my_list);
print first;

let rest = tail(my_list);
print rest;

# Test cons operator
let new_list = 0 :: my_list;
print new_list;

# Test empty and length
let is_empty = empty(my_list);
print is_empty;

let list_size = length(my_list);
print list_size;

# Test empty list
let empty_list = [];
print empty_list;

let is_empty_check = empty(empty_list);
print is_empty_check;

let empty_length = length(empty_list);
print empty_length;
